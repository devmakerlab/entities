{
    "name": "thephplab/entities",
    "type": "library",
    "description": "Work with entities from your services or repositories.",
    "license": "MIT",
    "keywords": [
        "entity",
        "repository",
        "pattern",
        "service"
    ],
    "authors": [
        {
            "name": "William",
            "email": "work@suppo.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Entities\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
